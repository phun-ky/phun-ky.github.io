    "js-yaml": "^4.1.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.6.2",
    "prettier-eslint": "^15.0.1",
